{
  "manifest_version": 2,

  "name": "True Yelp Reviews",
  "description": "Filter out Yelp reviews!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "yelp-icon-128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "yelp-icon-16.png",
    "48": "yelp-icon-48.png",
    "128": "yelp-icon-128.png"
  },
  "permissions": [
    "activeTab",
    "http://*.localhost/*",
    "http://192.168.4.106/*",
    "http://bid001-qa.aws.mgnt.cc/*"
  ]
}
